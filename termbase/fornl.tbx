    <descrip type="relatedConcept" target="eid-ordabanki-fornl-21">báshella</descrip>
    <langSet xml:lang="is">
     <tig>
      <term id="tid-ordabanki-fornl-30-is1">besla</term>
      <termNote type="grammaticalGender">feminine</termNote>
