    <langSet xml:lang="en">
     <tig>
      <term id="tid-ordabanki-lestrarsafn-714520-en1">writing instruction </term>
     </tig>
    </langSet>
   </termEntry>
   <termEntry id="eid-ordabanki-lestrarsafn-714521">
    <descrip type="definition">Vinnulag tengt ritun, t.d. réttritun, rithönd, nákvæmni og línubil.</descrip>
